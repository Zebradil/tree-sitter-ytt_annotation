{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "ytt_annotation",
      "camelcase": "YttAnnotation",
      "title": "YttAnnotation",
      "scope": "source.ytt_annotation",
      "file-types": [
        "ytt_annotation"
      ],
      "injection-regex": "^ytt_annotation$",
      "class-name": "TreeSitterYttAnnotation"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "YttAnnotation grammar for tree-sitter",
    "authors": [
      {
        "name": "German Lashevich",
        "email": "german.lashevich@gmail.com",
        "url": "https://github.com/zebradil"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-ytt_annotation"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}